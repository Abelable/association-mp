.navigation-bar {
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  align-items: center;
  width: 100%;
  height: 44px;
  background: #fff;
  box-sizing: initial;
  z-index: 50;
}
.navigation-title {
  margin-right: 40px;
  flex: 1;
  color: #333;
  font-size: 32rpx;
  text-align: center;
  font-weight: 500;
}