@import "/templates/i-empty-illus/index.wxss";
@import "/templates/i-youbo-logo/index.wxss";

page {
  background: #fff;
}

.course-list {
  position: relative;
  padding: 0 30rpx 160rpx;
  min-height: 100vh;
}

.course-item {
  border-bottom: 1rpx solid #F0F0F0;
}
.course-item:last-child {
  border-bottom: none;
}

.empty-illus-wrap {
  margin: 240rpx auto 0;
  width: fit-content;
}

.youbo-logo-wrap {
  position: absolute;
  bottom: 10rpx;
  width: calc(100vw -  60rpx);
}
