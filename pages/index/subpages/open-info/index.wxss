@import "/templates/i-empty-illus/index.wxss";

.open-info-list {
  position: relative;
  padding: 24rpx 24rpx 180rpx;
  min-height: 100vh;
}

.open-info-item {
  margin-bottom: 24rpx;
  padding: 0 24rpx;
  background: #fff;
  border-radius: 24rpx;
}
.open-info-item:last-child {
  border-bottom: none;
}

.empty-illus-wrap {
  margin: 240rpx auto 0;
  width: fit-content;
}

.youbo-logo-wrap {
  position: absolute;
  left: 50%;
  bottom: 0;
  transform: translate(-50%);
  margin-top: 100rpx;
}
