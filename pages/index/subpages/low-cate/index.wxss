@import "/templates/i-empty-illus/index.wxss";

page {
  background: #fff;
}

.container {
  position: relative;
  min-height: 100vh;
  padding-bottom: 160rpx;
}
.low-cate-list {
  display: flex;
  flex-wrap: wrap;
  padding: 24rpx;
}
.low-cate-item {
  margin-right: 18rpx;
  margin-bottom: 24rpx;
}
.low-cate-item:nth-child(3n) {
  margin-right: 0;
}

.empty-illus-wrap {
  margin: 240rpx auto 0;
  width: fit-content;
}

.youbo-logo-wrap {
  position: absolute;
  bottom: 10rpx;
  width: calc(100vw -  60rpx);
}
