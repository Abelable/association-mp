.think-list {
  margin-top: 20rpx;
  padding: 10rpx 20rpx 20rpx;
  background: #FFFFFF;
  border-radius: 12rpx;
}
.think-item {
  border-bottom: 1rpx solid #F0F0F0;
}

.more-btn {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 28rpx;
  color: #333;
  font-size: 26rpx;
}
.down-arrow {
  margin-left: 4rpx;
  width: 24rpx;
  height: 24rpx;
}