<view class="search-bar">
  <view class="search-input-wrap">
    <input class="search-input" value="{{ searchContent }}" bindinput="setSearchContent" placeholder="请输入您要搜索的内容" />
  </view>
  <view class="search-btn">搜索</view>
</view>

